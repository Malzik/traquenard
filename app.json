      "versionCode": 1,
      "permissions": []
    },
    "web": {
      "favicon": "./assets/logo_captain.png"
    },
    "androidNavigationBar": {
      "visible": "leanback",
      "backgroundColor": "#2A2A2A"
    }
  }
